{
  "name": "swr-devtools-community",
  "version": "0.2.0",
  "description": "Developer tools for SWR",
  "main": "dist/index.cjs.js",
  "module": "dist/index.es.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "sideEffects": false,
  "scripts": {
    "dev": "vite",
    "build": "vite build && tsc --emitDeclarationOnly --declaration --noEmit false --outDir dist",
    "lint": "eslint src --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview",
    "release": "npm run build && npm publish"
  },
  "peerDependencies": {
    "react": ">=16.8.0",
    "react-dom": ">=16.8.0",
    "swr": ">=1.0.0"
  },
  "devDependencies": {
    "@types/node": "^25.0.9",
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.0",
    "@vitejs/plugin-react": "^4.2.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "swr": "^2.2.0",
    "typescript": "^5.2.0",
    "vite": "^5.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/kudoas/swr-devtools"
  },
  "keywords": [
    "swr",
    "devtools",
    "react",
    "hooks",
    "cache",
    "mutation"
  ],
  "author": "kudoas",
  "license": "MIT",
  "dependencies": {
    "goober": "^2.1.14"
  }
}
